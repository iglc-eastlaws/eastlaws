@{
    Layout = null;
}
<div class="">
    <form class="search-form form-horizontal" ng-submit="search()" ng-click="formclick($event)">
        <div class="form-group">
            <div class="col-lg-6 col-md-12 col-side">
                <label class="col-lg-3 col-md-3 col-sm-3 control-label bg-color title" for="nassAll">
                    كافة النصوص :
                </label>
                <div class="col-lg-9 col-md-9 col-sm-9 search">
                    <c-drop-match-search selected-value="Inputs.alltextSearchType" input-text="Inputs.Alltext"></c-drop-match-search>
                </div>
                <hr class="spacer" />
                <label for="ddlCountries" class="col-lg-3 col-md-3 col-sm-3 control-label bg-color title"> اختيار الدولة :</label>
                <div class="col-lg-9 col-md-9 col-sm-9 search">
                    <select tabindex="3" class="string search  form-control" id="ddlCountries" name="d"
                            ng-options="t.Name for t in countries track by t.ID" ng-model="country" ng-change="updatecountry()"></select>
                </div>  <!--End Country-->
                <hr class="spacer" />
                <label class="col-lg-3 col-md-3 col-sm-3 control-label bg-color title">
                    المبادئ :
                </label>
                <div class="col-lg-9 col-md-9 col-sm-9 search">
                    <c-drop-match-search selected-value="Inputs.MabdaaSearchType"
                                         input-text="Inputs.Mabdaa"></c-drop-match-search>
                </div>


                <label class="col-lg-3 col-md-3 col-sm-3 control-label bg-color title">الوقائع : </label>
                <div class="col-lg-9 col-md-9 col-sm-9 search">
                    <c-drop-match-search selected-value="Inputs.waka23SearchType"
                                         input-text="Inputs.waka23"></c-drop-match-search>
                </div>


                <label class="col-lg-3 col-md-3 col-sm-3 control-label bg-color title">أعضاء الهيئة : </label>
                <div class="col-lg-9 col-md-9 col-sm-9 search">
                    <c-drop-match-search selected-value="Inputs.hay2aSearchType" input-text="Inputs.hay2a"></c-drop-match-search>
                </div>


                <label class="col-lg-3 col-md-3 col-sm-3 control-label bg-color title">حيثيات الحكم :  </label>
                <div class="col-lg-9 col-md-9 col-sm-9 search">
                    <c-drop-match-search selected-value="Inputs.hyseytSearchType" input-text="Inputs.hyseyt"></c-drop-match-search>
                </div>
          

                <label for="byan" class="col-lg-3 col-md-3 col-sm-3 control-label bg-color title">حالة الحكم  :</label>
                <div class="col-lg-9 col-md-9 col-sm-9 search">
                    <select tabindex="3" class="string search  form-control" id="byan"
                            ng-options="t.Name for t in MahkamaReplies track by t.ID" ng-model="MahkamaReplay" ng-change="updateMahkamaReplies()"></select>

                </div>
                <label for="issu" class="col-lg-3 col-md-3 col-sm-3 control-label bg-color title">جهة إصدار الحكم  :</label>
                <div class="col-lg-9 col-md-9 col-sm-9 search">
 
                    <select tabindex="3" class="string search  form-control" id="ddlCountries" name="d"
                            ng-options="t.Name for t in gehaType track by t.ID" ng-model="gehaTypeChoice" ></select>
                    @*<select tabindex="3" class="string search  form-control" id="issu">
                        <option value=""></option>
                        <option value=""></option>
                    </select>*@

                </div>
            </div>
            <div class="col-lg-6 col-lg-6">
                <div  class="form-group multi-check">
                    @*<label class="col-lg-3 col-md-3 col-sm-3 control-label bg-color title">اختيار المحكمة</label>*@

                    <div class="col-lg-12 col-md-12 col-sm-12  " >
                        <div class="checkbox" style="height:150px; ">
                            <fieldset ng-if="Mahakem.length>0">
                                <legend>
                                    <a href="javascript:;" ng-click="selectCheckboxes(1)">تحديد الكل</a> &nbsp;|&nbsp;
                                    <a href="javascript:;" ng-click="selectCheckboxes(0)">الغاء تحديد الكل</a>
                                </legend>
                                <div  class="search" style="height:120px; overflow-y:auto; overflow-x:hidden; padding:10px 12px 0;">
                                    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12" ng-repeat="n in Mahakem">
                                        <label><input class="ma7kama_checkbox" type="checkbox" ng-click='MahakemCheck(n.ID,$event)' />{{n.Name}} </label>
                                    </div>
                                   
                                </div>
                                <hr class="spacer" />

                            </fieldset>
                        </div>
                    </div>

                </div>
                <hr class="spacer" />
                <label for="yearTec" class="col-lg-3 col-md-3 col-sm-3 control-label bg-color title">  سنة المكتب الفنى : </label>
                <div class="col-lg-3 col-md-3 col-sm-3 col-sm-2 search">
                    <input type="text" class="form-control" id="yearTec" ng-model="Inputs.officeYear">
                </div>
                <label ng-show="omarGroupShow" for="amor" class="col-lg-3 col-md-3 col-sm-3 control-label bg-color title">  مجموعة عمر : <input type="checkbox" id="amor" ng-model="omarGroup" ng-click="omarGroupChecked($event)"></label>
                <div class="col-lg-3 col-md-3 col-sm-3 search checkbox">

                </div>
                <hr class="spacer" />
                <label for="ta3an" class="col-lg-3 col-md-3 col-sm-3 control-label bg-color title">  رقم الحكم : </label>
                <div class="col-lg-3 col-md-3 col-sm-3 search">
                    <input type="text" ng-model="Inputs.caseNo" class="form-control" id="ta3an">
                </div>
                <label class="col-lg-3 col-md-3 col-sm-3 control-label bg-color title" for="yearKada">السنة القضائية :  </label>
                <div class="col-lg-3 col-md-3 col-sm-3 search">
                    <input type="text" ng-model="Inputs.caseYear" class="form-control" id="yearKada">
                </div>
                <hr class="spacer" />
            
                <label for="horizontal_search" class="col-lg-3 col-md-3 col-sm-3 control-label bg-color title"> رقم الجزء :</label>
                <div class="col-lg-3 col-md-3 col-sm-3 search">
                    <input type="text" ng-model="Inputs.partNo" class="string   form-control">
                </div>

                <label for="pageNumber" class="col-lg-3 col-md-3 col-sm-3 control-label bg-color title"> رقم الصفحة : </label>
                <div class="col-lg-3 col-md-3 col-sm-3 search">
                    <input type="text" ng-model="Inputs.pageNo" class="string   form-control" id="pageNumber">
                </div>
                <hr class="spacer" />
                <label for="horizontal_search" class="col-lg-3 col-md-3 col-sm-3 control-label bg-color title"> تاريخ الجلسة من :</label>
                <div class="col-lg-3 col-md-3   search">
                    <c-date-picker date="Inputs.dateGalsaFrom"></c-date-picker>
                </div>

                <label for="horizontal_search" class="col-lg-3 col-md-3 col-sm-3 control-label bg-color title "> إلى: </label>

                <div class="col-lg-3 col-md-3   search">
                    <c-date-picker date="Inputs.dateGalsaTo"></c-date-picker>
                </div>


                <hr class="spacer" />
            </div>
            <hr class="spacer" />
     

            <hr class="spacer" />
            <div class="col-lg-6 col-side ">

                <hr class="spacer" />
                <label class="col-lg-3 col-md-3 col-sm-3 control-label bg-color title"> </label>
                <div class="col-sm-9 search">
                    <input type="submit" class="btn butt-yellow" value="بـحـث">
                </div>
            </div>

        </div>



    </form>
</div>
<hr class="spacer" />
